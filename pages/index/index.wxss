    border-radius: 10rpx;
    font-family: inherit;
    font-size: 28rpx;
    font-weight: normal;
    padding: 0 10rpx 0 60rpx;
    background-color: #fff;
    border: 1px solid #b4b4b4;
    color: #666;
}

.search-input .input::-webkit-input-placeholder {
    color: #cccccc;
    opacity: 1;
}

.search-input i {
    position: absolute;
