/* pages/my_cloud/submit_doc/submit_doc.wxss */
.has-address {
    font-size: 28rpx;
    line-height: 48rpx;
    height: 48rpx;
    display: flex;
}
.add{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 120px;
  height: 120px;
  border: 3px solid #fff;
  border-radius: 4rpx;
}
.doc-pic{
  position: relative;
  width: 100%;
  height: 100%;
}
.doc-pic image{
  width: 100%;
  height: 100%;
}
.doc-pic van-icon{
  position: absolute;
  top: 0;
  right: 0;
  transform: translate(50%,-50%);
}